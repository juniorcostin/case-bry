apiVersion: v1
kind: Config
clusters:
- name: "$K8S_NAMESPACE"
  cluster:
    server: "https://rancher.costin.com.br/k8s/clusters/$RANCHER_CLUSTER"

users:
- name: "$K8S_NAMESPACE"
  user:
    token: "RANCHER_TOKEN"


contexts:
- name: "$K8S_NAMESPACE"
  context:
    user: "$K8S_NAMESPACE"
    cluster: "$K8S_NAMESPACE"

current-context: "$K8S_NAMESPACE"